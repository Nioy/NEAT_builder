{
  "name": "neatbuilder",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run env_setup_development && webpack-dev-server --open --inline --hot",
    "prod": "npm run env_setup_production webpack --progress --hide-modules",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node tools/startServer.js",
    "env_setup_production": "node tools/setupEnv.js production",
    "server_production": "node -r dotenv/config tools/startServer.js",
    "env_setup_development": "node tools/setupEnv.js development",
    "server_development": "nodemon tools/startServer.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guiconti/NEAT_builder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guiconti/NEAT_builder/issues"
  },
  "homepage": "https://github.com/guiconti/NEAT_builder#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "bootstrap-vue": "^2.0.0-rc.11",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "p5": "^0.6.1",
    "rollbar": "^2.3.9",
    "vue": "^2.5.16",
    "vue-resource": "^1.5.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "morgan": "^1.9.0",
    "nodemon": "^1.17.4",
    "@vue/cli-plugin-babel": "^3.0.0-beta.10",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.10",
    "@vue/cli-service": "^3.0.0-beta.10",
    "vue-template-compiler": "^2.5.13",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "vue-loader": "^9.7.0",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.0"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
